generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model LastReleases {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  data      Json
  createdAd DateTime @default(now())
}

model mostPopular {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  data      Json
  createdAd DateTime @default(now())
}
